<t-swipe-cell>
  <t-cell title="Swipe Left" note="note" bordered="{{false}}" />
  <view slot="right" class="btn delete-btn" bind:tap="onDelete">Delete</view>
</t-swipe-cell>

<t-swipe-cell>
  <t-cell
    bordered="{{false}}"
    title="Swipe Left"
    description="description"
    note="note"
    image="https://tdesign.gtimg.com/mobile/demos/avatar1.png"
  />
  <view slot="right" class="btn delete-btn" bind:tap="onDelete">Delete</view>
</t-swipe-cell>

<t-swipe-cell right="{{right}}" bind:click="onActionClick">
  <t-cell title="Swipe Left" note="note" bordered="{{false}}" />
</t-swipe-cell>

<t-swipe-cell>
  <t-cell title="Swipe Left" note="note" bordered="{{false}}" />
  <view slot="right" class="btn-wrapper">
    <view class="btn favor-btn" bind:tap="onFavor">Collect</view>
    <view class="btn edit-btn" bind:tap="onEdit">Edit</view>
    <view class="btn delete-btn" bind:tap="onDelete">Delete</view>
  </view>
</t-swipe-cell>
