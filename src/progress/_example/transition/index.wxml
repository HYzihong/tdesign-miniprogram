<t-progress percentage="{{percentage}}" ariaLabel="{{ percentage + '%' }}" />

<view class="button-group">
  <t-button bindtap="clickReduce" theme="primary" variant="plain" size="small">Decrease</t-button>
  <view class="space"></view>
  <t-button bindtap="clickAdd" theme="primary" size="small">Increase</t-button>
</view>
