<t-tabs
  defaultValue="{{0}}"
  bind:change="onTabsChange"
  bind:click="onTabsClick"
  t-class="custom-tabs"
  t-class-content="custom-panel"
>
  <t-tab-panel label="Label 1" value="0" style="{{tabPanelstyle}}">Content 1</t-tab-panel>
  <t-tab-panel label="Label 2" value="1" style="{{tabPanelstyle}}">Content 2</t-tab-panel>
  <t-tab-panel label="Label 3" value="2" style="{{tabPanelstyle}}">Content 3</t-tab-panel>
  <t-tab-panel label="Label 4" value="3" style="{{tabPanelstyle}}">Content 4</t-tab-panel>
</t-tabs>
