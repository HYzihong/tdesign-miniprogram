<t-cell title="Choose Time" hover note="{{text || ''}}" arrow bindtap="showPicker" t-class="panel-item" />

<t-date-time-picker
  title="Choose Time"
  visible="{{visible}}"
  value="{{value}}"
  format="HH:mm:ss"
  mode="{{['null', 'second']}}"
  steps="{{ { minute: 5 } }}"
  bindchange="onConfirm"
  bindpick="onColumnChange"
  bindcancel="hidePicker"
/>
